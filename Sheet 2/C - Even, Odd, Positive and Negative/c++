#include <iostream>
using namespace std;
 
int main()
{int l;
cin>>l;
int num;
int count_even=0, count_odd=0, count_positive=0 , cout_negative=0;
for (int i =1; i<=l;i++){
    cin>> num;
    if (num>0){
         count_positive++;}
    else if (num<0){
       cout_negative++;}
       
       
       
    if (num%2==0){
       count_even++;}
    else if(num%2!=0){
         count_odd++;}
    
}
cout<< "Even: "<<count_even<<endl<<"Odd: "<<count_odd<<endl<<"Positive: "<<count_positive<<endl <<"Negative: "<<cout_negative;
 
    return 0;
}
