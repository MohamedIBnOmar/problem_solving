 
#include <iostream>
using namespace std;
int main()
{    int size;
    cin >>size;
    
    int arr[size]={} ;
    
   for (int i=0;i<size;i++){
       cin>>arr[i];
   }
   int indx,flag=0;
   cin>>indx;
   for (int i=0;i<=size;i++){
       if (indx==arr[i]){
             cout<<i<<endl;         
           flag=1;
           break;
           
       }
       
       
   }
   if (flag==0)              cout<<-1<<endl;         
 
   
    return 0;
}
